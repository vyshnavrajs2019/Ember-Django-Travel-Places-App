<Form::Signin
	@buttonTitle="Sign Up"
	@formTitle="Create Your Account"
	@username={{ @username }}
	@password={{ @password }}
	@handleSubmit={{ @handleSubmit }}>
		<Input
			@value={{ @confPassword }}
			type="password" 
			class="row sm-12 input input-md pl-1 pr-1 mb-075"
			placeholder="Confirm Password"></Input>
</Form::Signin>